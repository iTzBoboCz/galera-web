{
  "name": "galera-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:js": "eslint --ext .ts,.js,.json,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint",
    "*.{css,scss,vue}": "stylelint"
  },
  "dependencies": {
    "@galera/client-axios": "git+https://github.com/galera-org/galera-client-axios.git",
    "axios": "^0.24.0",
    "iso-639-1": "^2.1.11",
    "pinia": "^2.0.9",
    "pinia-plugin-persistedstate": "^1.0.3",
    "vue": "^3.2.26",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.0.12",
    "vuetify": "3.0.0-alpha.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^15.0.0",
    "@intlify/vite-plugin-vue-i18n": "^3.2.1",
    "@mdi/font": "^6.5.95",
    "@types/node": "^16.11.13",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vitejs/plugin-vue": "^2.0.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "sass": "~1.45.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.9",
    "vite-plugin-pages": "^0.18.0",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.30.1"
  }
}
