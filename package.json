{
  "name": "galera-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:js": "eslint --ext .ts,.js,.json,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint",
    "*.{css,scss,vue}": "stylelint"
  },
  "dependencies": {
    "@galera/client-axios": "git+https://github.com/galera-org/galera-client-axios.git",
    "axios": "^0.23.0",
    "iso-639-1": "^2.1.9",
    "vue": "^3.2.11",
    "vue-axios": "^3.2.5",
    "vue-i18n": "^9.1.7",
    "vue-router": "^4.0.11",
    "vuetify": "3.0.0-alpha.11",
    "vuex": "^4.0.2",
    "vuex-composition-helpers": "^2.0.0",
    "vuex-persistedstate": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@intlify/vite-plugin-vue-i18n": "^2.4.0",
    "@mdi/font": "^6.1.95",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vitejs/plugin-vue": "^1.6.2",
    "@vue/compiler-sfc": "^3.2.11",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "eslint-plugin-vue": "^7.17.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "sass": "~1.39.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.4.3",
    "vite": "^2.5.6",
    "vite-plugin-pages": "^0.18.0",
    "vite-plugin-vue-layouts": "^0.4.1",
    "vue-eslint-parser": "^7.11.0",
    "vue-tsc": "^0.3.0"
